%h1 Sign in
= form_for @user_session do |f|
  = render 'shared/error_messages', :object => f.object
  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :password
    = f.password_field :password
  .field
    = f.check_box :remember_me
    = f.label :remember_me
  .actions
    = f.submit "Login"
    
%p Not a member?
= link_to "Signup", signup_path